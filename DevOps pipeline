Yaml

name: DevOps CI Pipeline

on:[push,pull_request]

jobs:
 build:
  runs-on: ubuntu-latest

 steps:
  - name: Checkout code
    uses: actions/checkout@v4

  - name: set up Node.js
    uses: actions/setup-node@v4
    with:
     node-verdion: 18

  - name: install dependencies 
    run: npm install
  - name: Run test
    run: npm test
